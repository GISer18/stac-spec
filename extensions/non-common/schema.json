{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "schema.json#",
  "type": "object", 
  "title": "Non-Common Properties Extension Specification",
  "required": [
    "other_properties"
  ],
  "properties": {
    "other_properties": {
      "type": "object",
      "additionalProperties": {
        "type": "object",
        "properties": {
          "extent": {
            "type": "array",
            "minItems": 2,
            "maxItems": 2,
            "items": {
              "anyOf": [
                {
                  "type": "number"
                },
                {
                  "type": "string",
                  "format": "date-time"
                },
                {
                  "type": "null"
                }
              ]
            }
          },
          "values": {
            "type": "array",
            "items": {
              "anyOf": [
                {
                  "type": "number"
                },
                {
                  "type": "string"
                },
                {
                  "type": "string",
                  "format": "date-time"
                },
                {
                  "type": "boolean"
                },
                {
                  "type": "null"
                }
              ]
            }
          }
        }
      }
    }
  }
}